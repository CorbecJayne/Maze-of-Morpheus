package levels;


public class Level0 {
	public static int[][] getLevel() {
		int[][] result = new int[23][32];
		for (int x = 0; x < result.length; ++x) {
			for (int y = 0; y < result[0].length; ++y) {
				result[x][y] = 1;
			}
		}
		for (int x = 1; x < result.length-1; ++x) {
			for (int y = 1; y < result[0].length-1; ++y) {
				result[x][y] = 0;
			}
		}
		result[1][1] = 3;
		result[20][20] = 2;
		
		return result;
	}
}
